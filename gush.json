// Example Gitree.json file
{
  "clear_new_path": true,
  "paths": [
    {
      "repo_path": "*",
      "new_path": "/app/current"
    }
  ],
  "commands": [
    {
      "command": "bundle install --path vendor/bundle",
      "cwd": "/app/current"
    },
    {
      "command": "rake assets:precompile",
      "cwd": "/app/current"
    }
  ]
}
